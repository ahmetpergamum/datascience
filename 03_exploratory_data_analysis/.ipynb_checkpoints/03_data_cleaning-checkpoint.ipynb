    "dman = dc.copy()\n",
    "\n",
    "# function to fill manually\n",
    "def fillmanually(columns, pivotyear):\n",
    "    for column in columns:  \n",
    "        for state in dc.STATE.unique():\n",
    "            pivotpt = pivotyear\n",
    "            pivotval = dman.at[ str(pivotpt) + '_' + state , column]\n",
    "            for year in range(pivotyear-1,1985,-1):\n",
    "                diff = pivotpt - year\n",
    "                diffval = dman.at[str(pivotpt + diff) + '_'+ state , column]\n",
    "                dman.at[str(year)+ '_'+ state , column] = pivotval - (diffval - pivotval)\n",
    "\n",
    "            pivotpt = 2016\n",
    "            pivotval = dman.at[str(pivotpt)+ '_' + state , column]\n",
    "            for year in range(2017,2018):\n",
    "                diff = year - pivotpt\n",
    "                diffval = dman.at[str(pivotpt - diff) + '_'+ state , column]\n",
    "                dman.at[str(year)+ '_'+ state , column] = pivotval + (pivotval - diffval)\n",
    "\n",
    "\n",
    "# columns to fill manually\n",
    "columns = [ 'TOTAL_REVENUE', 'FEDERAL_REVENUE', 'STATE_REVENUE', 'LOCAL_REVENUE',\n",
    "           'TOTAL_EXPENDITURE', 'INSTRUCTION_EXPENDITURE', 'SUPPORT_SERVICES_EXPENDITURE', \n",
    "           'CAPITAL_OUTLAY_EXPENDITURE']\n",
    "\n",
    "# columns with 1992-2016 data\n",
    "fillmanually(columns,1992)\n",
    "# columns with 1993-2016 data\n",
    "fillmanually(['ENROLL','OTHER_EXPENDITURE'],1993)\n",
    "# columns with 1997-2016 data\n",
    "fillmanually(['GRADES_1_8_G', 'GRADES_9_12_G'],1997)\n",
    "\n",
    "# lineer interpolasyon\n",
    "\n",
    "columns = ['AVG_MATH_4_SCORE', 'AVG_MATH_8_SCORE','AVG_READING_4_SCORE', 'AVG_READING_8_SCORE', 'GRADES_PK_G']\n",
    "\n",
    "for column in columns:\n",
    "    dman[column].interpolate(method='linear', limit_direction = 'both',inplace=True)\n",
    "\n",
    "    \n",
    "dman.to_csv('states_all_clean_filled.csv')\n",
    "\n",
    "print('\\nCheck for the filled values\\n')\n",
    "\n",
    "state = 'CALIFORNIA'\n",
    "column = 'GRADES_1_8_G'\n",
    "\n",
    "display(dc.loc[dc['STATE']==state,[column]].head(8))\n",
    "display(dc.loc[dc['STATE']==state,[column]].tail(3))\n",
    "\n",
    "display(dman.loc[dman['STATE']==state,[column]].head(8))\n",
    "display(dman.loc[dman['STATE']==state,[column]].tail(3))\n",
    "print('\\nEksik degerler dolduruldu\\n')\n",
    "display(dman.isnull().mean()*100)\n",
    "\n",
    "ls = list(dc.loc[dc['STATE']==state][column])\n",
    "lsfilled = list(dman.loc[dman['STATE']==state][column])\n",
    "\n",
    "\n",
    "\n",
    "plt.figure(figsize=(24,8))\n",
    "plt.subplot(121)\n",
    "plt.axis(xmin = 1985, xmax=2018, ymin=0, ymax = 12000000)\n",
    "plt.title(state + ' '+ column + ' before filling NAs manually')\n",
    "plt.plot(dc.YEAR.unique(), ls )\n",
    "\n",
    "plt.subplot(122)\n",
    "plt.axis(xmin = 1985, xmax=2018, ymin=0, ymax = 12000000)\n",
    "plt.title(state + ' '+ column + ' after filling NAs manually')\n",
    "plt.plot(dc.YEAR.unique(),lsfilled)\n",
    "\n",
    "\n",
    "plt.show()\n",
    "\n",
    "dman.info()\n",
    "\n"
